    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Run with correct parameter",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["https://gametracker.rs"],
        },
        {
            "name": "Python: Run with incorrect parameter",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["dddddddd"]
        },
        {
            "name": "Python: Run with no cloudflare site",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["https://google.com"]
        },
        {
            "name": "Python: Run with too many arguments",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["https://gametracker.rs", "https://google.com"]
        }
    ]
}